list_availableSeats<- function(seatingChart){
seat_available <- vector("list",7)
for(y in 1:7){
map(1:9,
~{
if(seatingChart[[y]][[.x]]==0){
paste0("row",y,"-",.x)
}
})->seat_available[[y]]
}
unlist(seat_available) -> seat_available
return(seat_available)
}
list_availableSeats(seatingChart)
