update_seatingChart <- function(seatingChart, selectedSeat){
if(seatingChart==0){
for(b in 1:7){
map(1:9,
~{
if(seatingChart[[b]][[.x]]==-1){
paste0("row",b,"-",.x)
}
})->selectedSeat[[b]]
unlist(selectedSeat) -> seatingChart
} else {warning("很抱歉您所選的位置已無法選")}
